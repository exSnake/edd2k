{
  "manifest_version": 3,
  "name": "xEDD2k",
  "description": "EDD2k is a browser extension that allows you to grab ed2k links from the current tab.",	
  "author": "exSnake",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["https://*/*"],
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ]
}
